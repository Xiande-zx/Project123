<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ServicioListado">

    <include
        android:id="@+id/toolbar1"
        layout="@layout/toolbar"></include>

    <EditText
        android:id="@+id/editText9"
        android:layout_width="291dp"
        android:layout_height="51dp"
        android:background="@drawable/edittextredondos"

        android:drawableLeft="?android:attr/actionModeWebSearchDrawable"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:hint="@string/edittextBuscadorServicios2"
        android:inputType="textPersonName"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.141"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar1"
        app:layout_constraintVertical_bias="0.062" />

    <ListView
        android:id="@+id/lista"
        android:layout_width="378dp"
        android:layout_height="504dp"

        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        app:layout_constraintVertical_bias="0.112" />

    <Button
        android:id="@+id/button17"
        android:layout_width="51dp"
        android:layout_height="51dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:background="@drawable/btn_dos_ripple_user"
        android:drawableBottom="@drawable/user4"
        android:textColor="#17202A"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/editText9"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.117" />
</androidx.constraintlayout.widget.ConstraintLayout>